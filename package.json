{
  "name": "serper-search",
  "version": "0.1.0",
  "description": "serper search the internet",
  "private": true,
  "type": "module",
  "bin": {
    "serper-search": "./build/index.js"
  },
  "files": [
    "build"
  ],
  "scripts": {
    "build": "tsc && node -e \"require('fs').chmodSync('build/index.js', '755')\"",
    "prepare": "npm run build",
    "watch": "tsc --watch",
    "inspector": "npx @modelcontextprotocol/inspector build/index.js",
    "start": "node build/index.js",
    "start:http": "node build/index.js --http",
    "docker:build": "docker build -t serper-search .",
    "docker:run": "docker run -p 3000:3000 -e MCP_HTTP_MODE=true serper-search",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "0.6.0",
    "@types/axios": "^0.14.4",
    "@types/cheerio": "^0.22.35",
    "axios": "^1.7.9",
    "cheerio": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.17.10",
    "typescript": "^5.3.3"
  }
}
